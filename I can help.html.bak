	<script>
	
	// Handle form submission
const form = document.querySelector('form');
form.addEventListener('submit', async (event) => {
	event.preventDefault();
	
	// Get form data
	const typeOfHelp = document.getElementById('typeOfHelp').value;
	const categories = document.getElementById('categories').value;
	const location = document.getElementById('location').value;
	const schedule = document.getElementById('schedule').value;
	const comments = document.getElementById('comments').value;
	const categoriestype = document.getElementById('categoriestype').value;
	const locationtype = document.getElementById('location-type').value;
	const name = document.getElementById('name').value;
	const email = document.getElementById('email').value;
	
	// Create helper object
	const helper = {
		typeOfHelp,
		categories,
		location,
		schedule,
		comments,
		categoriestype,
		locationtype,
		name,
		email,
	};
	
	// Add helper to database
	try {
		const response = await fetch('https://backend-for-plans.onrender.com/api/helpers', {
			method: 'POST',
			headers: {
				'Content-Type': 'application/json'
			},
			body: JSON.stringify(helper)
		});
		
		if (!response.ok) {
			throw new Error(`HTTP error! status: ${response.status}`);
		}
		if (response.ok) {
			alert("Ok hogya");
		}
		
		const data = await response.json();
		console.log(data);
		// Clear form inputs
		form.reset();
	} catch (error) {
		console.error(error);
		alert('Failed to add helper. Please try again.');
	}
	

});

// Get helpers from database
async function getHelpers() {
	try {
		const response = await fetch('https://backend-for-plans.onrender.com/api/helpers');
		
		if (!response.ok) {
			throw new Error(`HTTP error! status: ${response.status}`);
		}
		
		const helpers = await response.json();
		const helpersList = document.getElementById('helpers-list');
		
		helpers.forEach(helper => {
			const li = document.createElement('li');
			li.innerHTML = `<h1>${helper.name}</h1> <br> <h2 style="margin-top:-13px">${helper.email}</h2> <br><hr style="height:1px;margin-top:-10px; background:#fff;">
				<p><strong style="margin-right:15px;" >Type of help:</strong> ${helper.typeOfHelp}</p>
				<p><strong style="margin-right:25px;">Categories:</strong> ${helper.categoriestype} - ${helper.categories}</p>
				<p><strong style="margin-right:41px;">Location:</strong> ${helper.locationtype} - ${helper.location}</p>
				<p><strong style="margin-right:37px;">Schedule:</strong> ${helper.schedule}</p>
				<p><strong style="margin-right:20px;">Description: </strong> ${helper.comments}</p>
				
			`;
			helpersList.appendChild(li);
		});
	} catch (error) {
		console.error(error);
		alert('Failed to get helpers. Please try again.');
	}
}


window.onload = function() {
getHelpers();
}
</script>

